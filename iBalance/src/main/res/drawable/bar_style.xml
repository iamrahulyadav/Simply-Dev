<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0dip" />
            <gradient
                android:angle="270"
                android:centerColor="#1ad3a2"
                android:centerY="0.5"
                android:endColor="#1ad3a2"
                android:startColor="#1ad3a2" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0dip" />
                <gradient
                    android:angle="0"
                    android:endColor="#feb52e"
                    android:startColor="#feb52e" />
            </shape>
        </clip>
    </item>
</layer-list>